{
  "name": "dungeoneer",
  "productName": "Dungeoneer",
  "version": "1.0.2",
  "description": "A tool for Dungeon Masters running 5th edition D&D games.",
  "main": "main.js",
  "scripts": {
    "start": "APP_DEV=true electron .",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always",
    "package": "electron-packager . Dungeoneer --icon=build/icon.ico --overwrite"
  },
  "author": "durtur@github ",
  "license": "CC-BY-NC-4.0",
  "devDependencies": {
    "electron": "^10.4.6",
    "electron-builder": "^20.44.4",
    "electron-packager": "^12.0.2",
    "electron-reload": "^1.5.0"
  },
  "dependencies": {
    "electron-log": "^4.3.4",
    "electron-prompt": "^1.3.1",
    "electron-updater": "^4.3.9",
    "file-system": "^2.2.2",
    "googleapis": "^73.0.0",
    "marked": "^2.0.5",
    "nouislider": "^14.0.2",
    "sharp": "^0.26.1",
    "turndown": "^5.0.3",
    "uniqid": "^5.2.0"
  },
  "postinstall": "electron-builder install-app-deps",
  "repository": {
    "url": "https://github.com/Durtur/Dungeoneer"
  }
}
